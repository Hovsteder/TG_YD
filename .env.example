# Основные настройки
NODE_ENV=production
SESSION_SECRET=your_secure_session_secret_here

# Настройки базы данных
# Для прямого развертывания на сервере используйте localhost вместо db
DATABASE_URL=postgres://postgres:postgres@localhost:5432/telegram_app

# Настройки Telegram бота
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
VITE_TELEGRAM_BOT_ID=your_telegram_bot_id_here

# Настройки порта сервера (по умолчанию 5000)
PORT=5000